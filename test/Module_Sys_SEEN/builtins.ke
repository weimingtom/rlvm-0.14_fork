// 5.6  Variable manipulation

// 5.6.1  Integers
int tmp = -1
intA[0] = rnd(0)
intA[1] = rnd(tmp)
intA[2] = rnd(tmp, tmp)
tmp = 10
intA[3] = rnd(-tmp, -1)
intA[4] = rnd(-tmp, tmp)
intA[5] = rnd(tmp, -tmp)

